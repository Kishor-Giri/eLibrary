<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="elibrary.project.com.elibrary">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/LoginTheme">
        <activity android:name=".ui.login.LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ui.register.RegisterActivity" />
        <activity android:name=".ui.dashboard.member.MemberDashboardActivity" />
        <activity android:name=".ui.dashboard.staff.StaffDashboardActivity" />
        <activity android:name=".ui.dashboard.staff.Users.UserListActivity" />
        <activity android:name=".ui.dashboard.staff.Books.BookListActivity" />
        <activity android:name=".ui.dashboard.staff.ReturnBook.ReturnBookActivity" />
        <activity android:name=".ui.dashboard.staff.IssueBook.IssueBookActivity" />
        <activity android:name=".ui.dashboard.staff.IssuedBookList.IssuedBookListActivity"></activity>
    </application>

</manifest>